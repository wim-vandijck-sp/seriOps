<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE sailpoint PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<sailpoint>
  <ImportAction name='merge'>
    <Configuration name="SystemConfiguration">
      <Attributes>
      	<Map>
		  <entry key="supportedLanguages">
			<value>
				<List>
					<String>en_US</String>
					<String>zh_CN</String>
					<String>nl_NL</String>
					<String>fr_FR</String>
					<String>de_DE</String>
					<String>it_IT</String>
					<String>ja_JP</String>
					<String>ko_KR</String>
					<String>pt_BR</String>
					<String>es_ES</String>
					<String>sv_SE</String>
					<String>tr_TR</String>
				</List>
			</value>
		 </entry>
          <entry key="pluginRelaxExportDeclarationEnforcement" value="true"/>
          <entry key="provisioningTransactionLogLevel" value="Success"/>
          <entry key="attachmentsEnabled" value="true"/>
      	  <entry key="timeMachineEnabled" value="true"/>
      	  <entry key="loginPassThrough">
      	    <value>
      	      <List>
            		<Reference class="sailpoint.object.Application"  name="Active Directory"/>
      	      </List>
      	    </value>
      	  </entry>
          <entry key="managerRequestControls">
            <value>
              <Map>
                <entry key="allowAccountOnlyRequests">
                  <value>
                    <Boolean>true</Boolean>
                  </value>
                </entry>
              </Map>
            </value>
          </entry>
      	  <entry key="promptForAuthQuestionAnswersAfterLogin">
      	    <value>
      	      <Boolean>true</Boolean>
      	    </value>
      	  </entry>
      	  <entry key="enableForgotPassword">
      	    <value>
      	      <Boolean>true</Boolean>
      	    </value>
      	  </entry>
      	  <entry key="lcmEnableFulltext">
      	    <value>
      	      <Boolean>true</Boolean>
      	    </value>
      	  </entry>
      	  <entry key="enableSelfServiceRegistration">
      	    <value>
      	      <Boolean>true</Boolean>
      	    </value>
      	  </entry>
      	  <entry key="defaultEmailFromAddress" value="iiq-noreply@sailpointdemo.com"/>
      	  <entry key="defaultEmailHost" value="seri.sailpointdemo.com"/>
      	  <entry key="certificationEmailTemplate" value="New Certification - HTML"/>
      	  <entry key="scopingEnabled">
      	    <value>
      	      <Boolean>true</Boolean>
      	    </value>
      	  </entry>
      	  <entry key="enableRoleSunAssign">
      	    <value>
      	      <Boolean>true</Boolean>
      	    </value>
      	  </entry>
          <entry key='quickLinkCategories'>
            <value>
              <List>
                <QuickLinkCategory
                  name="Admin Tools" messageKey="Admin Tools"
                  enabled="true" icon="quickLinkCategoryAdmin" />
                <QuickLinkCategory 
                  name="ITServices" messageKey="IT Services"
                  enabled="true" icon="quickLinkCategoryCustom" />
              </List>
            </value>
          </entry>
        </Map>
      </Attributes>
    </Configuration>
  </ImportAction>
</sailpoint>
