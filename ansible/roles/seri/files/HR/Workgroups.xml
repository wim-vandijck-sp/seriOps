    <Map>
      <entry key="email" value="ad-admins@sailpointdemo.com"/>
      <entry key="displayName" value="AD Admins"/>
    </Map>
  </Attributes>
  <Capabilities>
    <Reference class="sailpoint.object.Capability" name="ApplicationAdministrator"/>
  </Capabilities>
  <Owner>
    <Reference class="sailpoint.object.Identity" name="spadmin"/>
