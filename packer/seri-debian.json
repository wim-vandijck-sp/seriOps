      "memory": "{{ user `memory` }}",
      "output_directory": "{{ user `build_directory` }}/packer-{{user `template`}}-qemu",
      "shutdown_command": "echo 'vagrant' | sudo -S /sbin/shutdown -hP now",
      "ssh_password": "vagrant",
      "ssh_port": 22,
      "ssh_timeout": "10000s",
      "ssh_username": "vagrant",
      "type": "qemu",
      "vm_name": "{{ user `template` }}",
      "qemu_binary": "qemu-system-aarch64",
      "qemuargs": [
        [ "-m", "{{ user `memory` }}" ],
        [ "-display", "{{ user `qemu_display` }}" ]
      ]
    }
  ],
  "post-processors": [
    {
      "output": "{{ user `build_directory` }}/{{user `box_basename`}}.{{.Provider}}.box",
      "keep_input_artifact":true,
      "type": "vagrant"
    }
  ],
  "provisioners": [
    {
      "environment_vars": [
        "HOME_DIR=/home/sailpoint",
        "http_proxy={{user `http_proxy`}}",
        "https_proxy={{user `https_proxy`}}",
        "no_proxy={{user `no_proxy`}}"
      ],
      "execute_command": "echo 'sailpoint' | {{.Vars}} sudo -S -E sh -eux '{{.Path}}'",
      "expect_disconnect": true,
      "scripts": [
        "scripts/debian/update.sh",
        "ansible.sh",
        "motd.sh",
        "scripts/common/sshd.sh",
        "scripts/debian/networking.sh",
        "scripts/debian/sudoers.sh",
        "scripts/common/vagrant.sh",
        "scripts/debian/systemd.sh",
        "scripts/common/virtualbox.sh",
        "scripts/common/vmware.sh",
        "scripts/common/parallels.sh",
        "scripts/debian/cleanup.sh",
        "scripts/common/minimize.sh"
      ],
      "type": "shell"
    },
    {
      "type": "file",
      "source": "../ansible/vault-password-file",
      "destination": "/tmp/vault-password-file"
    }
  ],
  "variables": {
